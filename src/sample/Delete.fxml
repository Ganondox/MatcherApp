<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DeleteController">
   <children>
      <Button fx:id="deleteYesButton" layoutX="114.0" layoutY="175.0" mnemonicParsing="false" onMouseClicked="#deleteProfile" text="Yes" />
      <Button fx:id="deleteNoButton" layoutX="202.0" layoutY="175.0" mnemonicParsing="false" onMouseClicked="#toBase" text="No" />
      <Label layoutX="38.0" layoutY="88.0" text="Are you sure? Saying yes will delete your profile." />
   </children>
</AnchorPane>
